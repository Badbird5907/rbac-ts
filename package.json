{
  "name": "rbac-ts",
  "version": "2.1.0",
  "description": "Blazing Fast, Zero dependency, Hierarchical Role-Based Access Control for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "./src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "examples": "node ./examples/index.js",
    "test": "ts-mocha -n loader=ts-node/esm -p tsconfig.json test/**/*.spec.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Badbird5907/rbac-ts.git"
  },
  "keywords": [
    "rbac",
    "authorization",
    "hierarchical",
    "privacy",
    "role",
    "security",
    "permission",
    "acl"
