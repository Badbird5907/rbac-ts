{
  "name": "rbac-ts",
  "version": "2.0.0",
  "description": "Blazing Fast, Zero dependency, Hierarchical Role-Based Access Control for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "./src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "webpack --progress --colors --watch --env dev",
    "examples": "node ./examples/index.js",
    "test": "cross-env NODE_ENV=test mocha --colors ./test/*.spec.js",
    "test:watch": "cross-env NODE_ENV=test mocha --colors -w ./test/*.spec.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Badbird5907/rbac-ts.git"
  },
  "keywords": [
    "rbac",
    "authorization",
    "hierarchical",
    "privacy",
    "role",
    "security",
    "permission",
    "acl"
  ],
  "author": "Evan Yu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Badbird5907/rbac-ts/issues"
  },
  "homepage": "https://github.com/Badbird5907/rbac-ts",
  "devDependencies": {
    "chai": "^5.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "mocha": "^10.7.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca"
}
